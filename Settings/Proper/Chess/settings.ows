		Call Subroutine(AddMoves);
		Event Player.CandidateMoveDelta = Vector(1, -1, 0);
		Call Subroutine(AddMoves);
		Event Player.CandidateMoveDelta = Vector(-1, -1, 0);
		Call Subroutine(AddMoves);
		Event Player.CandidateMoveDelta = Vector(-1, 1, 0);
		Call Subroutine(AddMoves);
	}
}

